<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.back.mapper.WxhandlerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.back.entity.pojo.Wxhandler">
        <id column="id" property="id" />
        <result column="keywords" property="keywords" />
        <result column="content" property="content" />
        <result column="channel" property="channel" />
        <result column="extra" property="extra" />
        <result column="create_person" property="createPerson" />
        <result column="create_time" property="createTime" />
        <result column="update_person" property="updatePerson" />
        <result column="update_time" property="updateTime" />
        <result column="dr" property="dr" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, keywords, content, channel, extra, create_person, create_time, update_person, update_time, dr
    </sql>

    <select id="page" resultType="com.back.entity.pojo.Wxhandler">
        select * from wxhandler
        <where>
            <if test="keywords != null and keywords != ''">
                AND keywords = #{keywords}
            </if>
            <if test="channel != null and channel != ''">
                AND channel = #{channel}
            </if>
        </where>
    </select>

</mapper>
